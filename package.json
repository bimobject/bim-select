{
  "name": "bim-select",
  "version": "2.3.1",
  "description": "A dropdown/select solution for Angular.js that handles millions of items without lag.",
  "main": "dist/bim-select.min.js",
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run lint && npm run test:unit -- --coverage",
    "test:unit": "karma start",
    "test:unit:watch": "karma start --no-single-run --auto-watch",
    "lint": "eslint src/*.js",
    "build": "node ./tools/bower-sync.js && yarn run build:dev && yarn run build:prod",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "changelog": "node ./tools/bower-sync.js && conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-recommended-bump -p angular"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bimobject/bim-select"
  },
  "sideEffects": false,
  "keywords": [
    "angular",
    "dropdown",
    "select",
    "virtual",
    "scroll"
  ],
  "author": "Glenn Jorde <glenn.jorde@bimobject.com> (https://github.com/bimobject)",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/runtime": "^7.0.0-beta.44",
    "angular-mocks": "^1.6.4",
    "babel-loader": "^8.0.0-beta",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "conventional-changelog-cli": "^2.0.1",
    "conventional-recommended-bump": "^4.0.0",
    "css-loader": "^1.0.0",
    "es6-promise": "^4.1.0",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-loader": "^0.5.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.4",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^3.0.0",
    "less": "^3.5.3",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "ngtemplate-loader": "^2.0.1",
    "puppeteer": "^1.5.0",
    "sinon": "^6.1.3",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "angular": "^1.5.11",
    "angular-vs-repeat": "^2.0.9",
    "jquery": ">=2"
  }
}
