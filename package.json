{
  "name": "bim-select",
  "version": "1.0.1",
  "description": "A dropdown/select solution for angular 1 that handles millions of items without lag.",
  "main": "dist/bim-select.js",
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "karma start",
    "test:unit:watch": "karma start --no-single-run --auto-watch",
    "lint": "eslint src/*.js",
    "build": "node ./tools/bower-sync.js && webpack",
    "changelog": "node ./tools/bower-sync.js && conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "conventional-recommended-bump -p angular"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bimobject/bim-select"
  },
  "keywords": [
    "angular",
    "dropdown",
    "select",
    "virtual",
    "scroll"
  ],
  "author": "Glenn Jorde <glenn@tregusti.com> (https://github.com/tregusti)",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.1.0",
    "conventional-changelog-cli": "^1.3.1",
    "conventional-recommended-bump": "^1.0.0",
    "css-loader": "^0.28.4",
    "es6-promise": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.3.1",
    "html-loader": "^0.4.5",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-dom": "^1.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-edge-launcher": "^0.4.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-nightmare": "^0.4.5",
    "karma-sinon-chai": "^1.2.4",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mocha": "^3.2.0",
    "ngtemplate-loader": "^1.3.1",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "angular": "^1.5.11",
    "angular-vs-repeat": "^1.1.7",
    "jquery": ">=2"
  }
}
